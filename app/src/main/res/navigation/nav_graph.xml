            app:destination="@id/conversion_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/fragmentInterpolationMenu"
            app:destination="@id/interpolation_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/fragmentLocationOfRootsMenu"
            app:destination="@id/location_of_roots_menu"
            app:enterAnim="@anim/slide_in_right"
